{
  "name": "firebaseui-angular-en-es",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-en-es-library --prod && npx copy README.MD dist/firebaseui-angular-en-es-library",
    "prerelease": "cd projects/firebaseui-angular-en-es-library && npm version prerelease --preid=beta",
    "_prepatch": "cd projects/firebaseui-angular-en-es-library && npm version prepatch  --preid=beta",
    "patch": "cd projects/firebaseui-angular-en-es-library && npm version patch",
    "_preminor": "cd projects/firebaseui-angular-en-es-library && npm version preminor  --preid=beta",
    "minor": "cd projects/firebaseui-angular-en-es-library && npm version minor",
    "_premajor": "cd projects/firebaseui-angular-en-es-library && npm version premajor  --preid=beta",
    "major": "cd projects/firebaseui-angular-en-es-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish dist/firebaseui-angular-en-es-library",
    "publish-dist-beta": "npm run build-lib && npm publish dist/firebaseui-angular-en-es-library --tag beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/fire": "^5.3.0",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "core-js": "3.3.3",
    "eslint": "^5.16.0",
    "firebase": "^6.6.2",
    "firebaseui-en-es": "git+https://github.com/appdev-tat/firebaseui-web-en-es.git",
    "rxjs": "6.5.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.2",
    "codelyzer": "~5.0.0",
    "copy": "^0.3.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tsickle": "^0.33.1",
    "tslib": "1.9.3",
    "tslint": "~5.15.0",
    "typescript": "3.4.5"
  }
}
